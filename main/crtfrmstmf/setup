#!/QOpenSys/usr/bin/sh
export PWD=$(pwd)
system -kpieb "CRTSRCPF FILE(OSSILE/CRTFRMSTMF) RCDLEN(198)"
system -kpieb "CPYFRMSTMF FROMSTMF('$PWD/crtfrmstmf.rpgle') TOMBR('/QSYS.lib/OSSILE.lib/CRTFRMSTMF.file/RPGLE.mbr') MBROPT(*REPLACE)"
system -kpieb "CPYFRMSTMF FROMSTMF('$PWD/crtfrmstmf.pnlgrp') TOMBR('/QSYS.lib/OSSILE.lib/CRTFRMSTMF.file/PNLGRP.mbr') MBROPT(*REPLACE)"
system -kpieb "CPYFRMSTMF FROMSTMF('$PWD/crtfrmstmf.cmd') TOMBR('/QSYS.lib/OSSILE.lib/CRTFRMSTMF.file/CMD.mbr') MBROPT(*REPLACE)"
system -kpieb "CRTBNDRPG PGM(OSSILE/CRTFRMSTMF) SRCFILE(OSSILE/CRTFRMSTMF) SRCMBR(RPGLE) REPLACE(*YES) TEXT('CRTFRMSTMF Program')"
system -kpieb "CRTPNLGRP PNLGRP(OSSILE/CRTFRMSTMF) SRCFILE(OSSILE/CRTFRMSTMF) SRCMBR(PNLGRP) REPLACE(*YES) TEXT('CRTFRMSTMF Panel Group')"
system -kpieb "CRTCMD CMD(OSSILE/CRTFRMSTMF) PGM(OSSILE/CRTFRMSTMF) SRCFILE(OSSILE/CRTFRMSTMF) SRCMBR(CMD) TEXT('CRTFRMSTMF Command') HLPPNLGRP(OSSILE/CRTFRMSTMF) HLPID(*CMD) REPLACE(*YES)"
system -kpieb "DLTOBJ OBJ(OSSILE/CRTFRMSTMF) OBJTYPE(*FILE)"
